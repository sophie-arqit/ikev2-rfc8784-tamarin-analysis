lemma IKE_SA_injective_agreement_I[use_induction]:
     "All spiI spiR nI nR I R Keys #i #j #k .
	IKE_SA_CompletedTightI(spiI, spiR, nI, nR, I, R, Keys) @#i
 	& IKE_SA_CompletedTightR(spiI, spiR, nI, nR, I, R, Keys) @ #j
    	& #j < #i // the legitimate #unique run
	& IKE_SA_CompletedTightR(spiI, spiR, nI, nR, I, R, Keys) @ #k // the additional run
    ==> #j = #k"

lemma IKE_SA_injective_agreement_I_v2:
     "All spiI spiR nI nR I R Keys #i.
	IKE_SA_CompletedTightI(spiI, spiR, nI, nR, I, R, Keys) @#i
